{
  "name": "project-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config jest.database.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.database.config.js --testPathPattern=integration --runInBand",
    "test:e2e": "jest --config jest.database.config.js --testPathPattern=e2e --runInBand",
    "test:security": "jest --config jest.database.config.js --testPathPattern=security",
    "test:database-complete": "npm run test:unit && npm run test:security && npm run test:integration && npm run test:e2e",
    "test:database": "./scripts/test-database.sh",
    "test:enum": "./scripts/test-enum.sh --all",
    "test:user-interface": "./scripts/test-user-interface.sh --all",
    "test:pagination": "jest --config jest.config.pagination.js",
    "test:config": "jest app.config.spec.ts",
    "test:cache": "./scripts/test-cache.sh all",
    "test:decorators": "./scripts/test-current-user.sh full",
    "test:guard": "./scripts/test-auth-guard.sh",
    "test:guard1": "./scripts/test-auth-guard.sh --unit-only",
    "test:guard2": "./scripts/test-auth-guard.sh --performance-only",
    "test:guard3": "./scripts/test-auth-guard.sh --security-only",
    "test:guard4": "./scripts/test-auth-guard.sh --integration-only",
    "test:guard5": "./scripts/test-auth-guard.sh --coverage-only",

    "test:project-owner": "./scripts/test-project-owner-guard.sh",
    "test:project-owner-unit": "npm test test/unit/common/guards/project-owner.guard.spec.ts",
    "test:project-owner-security": "npm test test/unit/common/guards/project-owner.guard.security.spec.ts",
    "test:project-owner-integration": "REDIS_MIN_CONNECTIONS=1 REDIS_MAX_CONNECTIONS=10 npm test test/integration/common/guards/project-owner.guard.integration.spec.ts",
    "test:project-owner-performance": "REDIS_MIN_CONNECTIONS=1 REDIS_MAX_CONNECTIONS=10 npm test test/performance/common/guards/project-owner.guard.performance.spec.ts",
    "test:project-owner-e2e": "jest --config jest.project-owner-guard.e2e.config.js",
    "test:project-owner-config": "npm run test -- --config=jest.project-owner-guard.config.js --verbose",
    "test:project-owner-all": "npm run test:project-owner-unit && npm run test:project-owner-security && npm run test:project-owner-integration && npm run test:project-owner-performance && npm run test:project-owner-e2e",

    "test:exception-utils": "./scripts/test-exceptions-utils.sh",

    "test:dto": "jest --config jest.create-project-dto.config.js",
    "test:dto:unit": "jest --config jest.create-project-dto.config.js --testPathPattern=unit",
    "test:dto:integration": "jest --config jest.create-project-dto.config.js --testPathPattern=integration",
    "test:dto:e2e": "jest --config jest.create-project-dto.config.js --testPathPattern=e2e",
    "test:dto:security": "jest --config jest.create-project-dto.config.js --testPathPattern=security",
    "test:dto:performance": "jest --config jest.create-project-dto.config.js --testPathPattern=performance",
    "test:dto:edge": "jest --config jest.create-project-dto.config.js --testPathPattern=edge-cases",

    "test:project-response-dto": "jest --config=jest.project-response-dto.config.js",

    "test:update-project-dto": "./scripts/test-update-project-dto.sh",

    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.12.0",
    "axios": "^1.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^5.4.0",
    "ioredis": "^5.6.1",
    "node-pandoc": "^0.3.0",
    "prisma": "^6.12.0",
    "redis": "^5.6.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.5",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^4.0.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "prisma-dbml-generator": "^0.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
